    "node-sass": "^4.11.0",
    "npm-license-crawler": "^0.1.5",
    "require-dir": "^0.3.0",
    "sass-loader": "^6.0.5",
    "spectron": "^3.6.4",
    "style-loader": "^0.18.1",
    "tree-kill": "^1.1.0",
    "url-loader": "^0.5.7",
    "vue-html-loader": "^1.2.2",
    "vue-loader": "^12.1.0",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.2.2",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0",
