{
  "name": "collate",
  "version": "0.4.0",
  "description": "A privacy focused, locally hosted, cross-platform note-taking application",
  "homepage": "https://collatenotes.com",
  "main": "./dist/electron/main.js",
  "scripts": {
    "publish": "npm run pack && build -ml -p always && build -w --x64 --ia32 -p always",
    "build": "node .electron-vue/build.js && build",
    "build:dir": "node .electron-vue/build.js && build --dir",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "build:win64": "npm run pack && build -w --x64 -p always",
    "build:win32": "npm run pack && build -w --ia32 -p always",
    "build:mac": "npm run pack && build -m -p always",
    "build:linux": "npm run pack && build -l -p always",
    "dev": "node .electron-vue/dev-runner.js",
    "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter src test",
    "lint:fix": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix src test",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "postinstall": "npm run lint:fix",
    "scrape:licenses": "node ./node_modules/.bin/npm-license-crawler --json ./src/renderer/components/About/licenses.json --dependencies"
  },
  "build": {
    "productName": "Collate",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron",
      "node_modules/",
      "package.json"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "icons/icon.icns",
      "category": "public.app-category.tools"
    },
    "win": {
      "icon": "icons/icon.ico",
      "publisherName": "Collate, LLC",
      "target": [
        "nsis",
        "portable"
      ]
    },
    "linux": {
      "icon": "icons",
      "category": "Office",
      "target": [
        "deb",
        "rpm",
        "AppImage",
        "tar.gz"
      ]
    }
  },
  "author": "Patrick Lee <patrick@collatenotes.com>",
  "license": "MIT",
  "dependencies": {
    "animate.css": "^3.5.2",
    "axios": "^0.16.1",
    "bulma": "^0.4.1",
    "cheerio": "^0.22.0",
    "deepmerge": "^1.3.2",
    "electron-in-page-search": "^1.2.3",
    "electron-localshortcut": "^2.0.2",
    "electron-log": "^2.2.6",
    "electron-settings": "^3.0.14",
    "electron-updater": "^2.10.0",
    "electron-window-state": "^4.1.1",
    "enex-parser": "^1.1.2",
    "font-awesome": "^4.7.0",
    "fs-extra": "^3.0.1",
    "gray-matter": "^2.1.1",
    "highlight.js": "^9.11.0",
    "html-to-text": "^3.2.0",
    "katex": "^0.7.1",
    "lunr": "1.0.0",
    "md5-file": "^3.1.1",
    "md5-hex": "^2.0.0",
    "moment": "^2.18.1",
    "pretty": "^2.0.0",
    "quill": "^1.2.4",
    "showdown": "^1.6.4",
    "simplemde": "^1.11.2",
    "striptags": "^3.0.1",
    "vue": "^2.3.3",
    "vue-clickaway": "^2.1.0",
    "vue-electron": "^1.0.6",
    "vue-resource": "^1.3.1",
    "vue-router": "^2.5.3",
    "vuex": "^2.3.1",
    "website-scraper": "^2.4.1"
  },
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-istanbul": "^4.1.1",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-env": "^1.3.3",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.2.0",
    "babel-runtime": "^6.6.1",
    "babili-webpack-plugin": "^0.1.1",
    "cfonts": "^1.1.3",
    "chai": "^3.5.0",
    "chalk": "^1.1.3",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.0",
    "css-loader": "^0.28.2",
    "del": "^2.2.1",
    "devtron": "^1.1.0",
    "electron": "^1.7.1",
    "electron-builder": "^19.31.1",
    "electron-debug": "^1.1.0",
    "electron-devtools-installer": "^2.0.1",
